  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "analyzer debug",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-analyzer",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "analyzer release",
      "program": "${workspaceFolder}/build/Release/bin/ticket-analyzer",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build release"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "decoder debug",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-decoder",
      "args": [
        "-i",
        "images/UIC918-9-2022/Muster 918-9 Normalpreis.png"
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "test debug",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-decoder-test",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    }
  ]
}